---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>janmonschke.com</title>
    <link>http://janmonschke.com/</link>
    <atom:link href="http://janmonschke.com/rss.xml" rel="self" type="application/rss+xml" />
    <description>Jan is blogging about tech'n stuff</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
      {% unless post.draft %}
        <item>
          <title>{{ post.title }}</title>
          <link>http://janmonschke.com{{ post.url }}</link>
          <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
          <guid>http://janmonschke.com{{ post.id }}</guid>
          <description>{{ post.content | xml_escape }}</description>
        </item>
      {% endunless %}
    {% endfor %}

  </channel>
</rss>