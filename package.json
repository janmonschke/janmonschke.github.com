    "gatsby-remark-smartypants": "^3.1.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^3.1.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-typography": "^0.16.20",
